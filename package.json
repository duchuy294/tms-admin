{
  "name": "tms-admin",
  "version": "1.1.0",
  "description": "TMS admin",
  "author": "OVTeam",
  "license": "MIT",
  "scripts": {
    "build": "ng build --output-hashing=all",
    "build:DEV1": "./node_modules/.bin/ng build --output-hashing=all --configuration=dev1",
    "build:DEV2": "./node_modules/.bin/ng build --output-hashing=all --configuration=dev2",
    "build:UAT": "./node_modules/.bin/ng build --output-hashing=all --configuration=uat",
    "build:PROD": "node --max_old_space_size=8096 ./node_modules/.bin/ng build --output-hashing=all --prod",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint-staged": "lint-staged",
    "lint:ci": "npm run lint && npm run lint:styles",
    "lint:styles": "stylelint ./src/app/**/*.scss",
    "ng": "ng",
    "start": "ng serve --open",
    "start:prod": "ng serve --prod",
    "start:prod:aot": "ng serve --prod --aot",
    "start:dev1": "npm run start -- --configuration=dev1",
    "start:dev2": "npm run start -- --configuration=dev2",
    "start:uat": "npm run start -- --configuration=uat"
  },
  "private": true,
  "dependencies": {
    "@adonisjs/websocket-client": "^1.0.9",
    "@agm/core": "^1.1.0",
    "@agm/snazzy-info-window": "^1.1.0",
    "@angular/animations": "^11.0.2",
    "@angular/cli": "^11.0.2",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "^11.0.2",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/platform-server": "^11.0.2",
    "@angular/router": "^11.0.2",
    "@babel/polyfill": "^7.4.4",
    "@ng-bootstrap/ng-bootstrap": "^5.0.0",
    "@ngrx/effects": "^8.5.2",
    "@ngrx/store": "^8.5.2",
    "@ngrx/store-devtools": "^8.5.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "agm-direction": "^0.7.9",
    "angular2-text-mask": "^9.0.0",
    "bootstrap": "^4.4.1",
    "core-js": "2.4.1",
    "express": "^4.17.1",
    "firebase": "^7.15.2",
    "font-awesome": "^4.7.0",
    "highcharts": "7.1.1",
    "highcharts-angular": "^2.4.0",
    "ionicons": "^2.0.1",
    "jquery": "^3.5.1",
    "jquery-slimscroll": "1.3.8",
    "lodash": "^4.17.14",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "ng-lazyload-image": "^6.0.0",
    "ng-zorro-antd": "^10.1.2",
    "ngx-ckeditor": "^8.0.0",
    "ngx-infinite-scroll": "^8.0.1",
    "popper.js": "^1.16.0",
    "resize-base64": "^1.0.12",
    "rxjs": "6.6.3",
    "rxjs-compat": "6.5.2",
    "snazzy-info-window": "^1.1.0",
    "text-mask-addons": "^3.8.0",
    "tslib": "^2.0.0",
    "xlsx": "^0.16.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular-devkit/build-optimizer": "^0.900.6",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@types/googlemaps": "^3.30.9",
    "@types/jasmine": "~3.6.0",
    "@types/jquery": "2.0.41",
    "@types/jquery.slimscroll": "1.3.30",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.20.45",
    "babel-polyfill": "^6.26.0",
    "codelyzer": "^6.0.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "4.0.2",
    "nz-tslint-rules": "^0.901.2",
    "prettier": "^1.19.1",
    "stylelint": "7.10.1",
    "ts-node": "8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "4.0.5"
  },
  "lint-staged": {
    "*.ts": [
      "node ng-lint-staged.js lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
